                    <i class="fa fa-area-chart" style="color:white">
                        Product  >>>@Session["DatetoExpire"]
                    </i>
                }
                else if (Session["UserType"].ToString() == "4")
                {
                    <i class="fa fa-area-chart" style="color:white">
                        Sales Man  >>>@Session["DatetoExpire"]
                    </i>
                }
                else if (Session["UserType"].ToString() == "5")
                {
                    <i class="fa fa-area-chart" style="color:white">
