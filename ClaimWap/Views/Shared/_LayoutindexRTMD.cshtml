    <div class="content-wrapper">
        <div class="container-fluid">
            <div class="row">
                <div class="col-xl-3 col-sm-6 mb-3">
                    <div class="card text-white bg-primary o-hidden" id="divMenuMain">
                        <a class="card-footer text-white clearfix small z-1" id="SelectDisplay" href="~/SelectDisplay/Index">
