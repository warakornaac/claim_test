    <add key="webpages:Version" value="2.0.0.0"/>
    <add key="webpages:Enabled" value="false"/>
    <add key="PreserveLoginUrl" value="true"/>
    <add key="ClientValidationEnabled" value="true"/>
    <add key="UnobtrusiveJavaScriptEnabled" value="true"/>
    <add key="aspnet:MaxJsonDeserializerMembers" value="55000000"/>
    <add key="CLAIM_ConnectionString" value="Data Source=training;Initial Catalog=Claim_test;Persist Security Info=True;User ID=plu;Password=access;MultipleActiveResultSets=True;Application Name=EntityFramework"/>
